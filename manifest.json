{
    "name": "WordDef",
    "description": "Get meaning on the fly",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches":[
                "http://*/*",
                "https://*/*"
            ],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "manifest_version": 2
}